{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Server=localhost;Port=5411;Database=neutrolab;User Id=postgres;Password=postgres;Pooling=true;MinPoolSize=1;MaxPoolSize=100;"
  },
  "NHibernate": {
    "ConnectionStringName": "Default",
    "Properties": {

      "connection.isolation": "ReadCommitted",
      "command_timeout": "90",
      "adonet.batch_size": "0",
      "show_sql": "true",
      "format_sql": "true",
      "generate_statistics": "true",
      "cache.use_second_level_cache": "true",
      "cache.use_query_cache": "true",
      "cache.provider_class": "NHibernate.Cache.HashtableCacheProvider, NHibernate",
      "hbm2ddl.keywords": "auto-quote",
      "connection.provider": "NHibernate.Connection.DriverConnectionProvider",

      "default_schema": "public",
      "dialect": "NHibernate.Dialect.PostgreSQLDialect",
      "connection.driver_class": "NHibernate.Driver.NpgsqlDriver"

    }
  }
}
